{
  "name": "yiisoft/yii2-app-basic",
  "description": "Yii 2 Basic Project Template",
  "keywords": [
    "yii2",
    "framework",
    "basic",
    "project template"
  ],
  "homepage": "http://www.yiiframework.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": ">=2.0.3",
    "yiisoft/yii2-bootstrap": "*",
    "yiisoft/yii2-swiftmailer": "*",
    "nodge/yii2-eauth": "~2.0",
    "gregwar/image": "2.*",
    "kartik-v/yii2-widget-typeahead": "*",
    "fxp/composer-asset-plugin": "~1.0",
    "2amigos/yii2-tinymce-widget": "~1.1",
    "yii-cms/yii2-robokassa": "1.0.0",
    "bootstrap-select/bootstrap-select": "^1.7",
    "himiklab/yii2-sitemap-module": "*",
    "sunra/php-simple-html-dom-parser": " v1.5.0",
    "components/jquery": "^2.2",
    "electrolinux/phpquery": "^0.9.6",
    "vova07/yii2-imperavi-widget": "^1.2",
    "dmstr/yii2-adminlte-asset": "^2.2",
    "kartik-v/yii2-widget-datetimepicker": "*",
    "nill/forum": "dev-master",
    "bupy7/yii2-bbcode": "1.*",
    "kartik-v/yii2-widget-datepicker": "@dev",
    "yiisoft/yii2-imagine": "^2.0",
    "kartik-v/yii2-widget-depdrop": "@dev",
    "codeception/specify": "*",
    "codeception/codeception": "2.0.*",
    "codeception/verify": "*",
    "froala/yii2-froala-editor": "dev-master"
  },
  "require-dev": {
    "udokmeci/yii2-phone-validator": "dev-master",
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-jui": "*",
    "yiisoft/yii2-faker": "*",
    "marciocamello/yii2-paypal": "*"
  },
  "config": {
    "process-timeout": 1800
  },
  "scripts": {
    "post-create-project-cmd": [
      "yii\\composer\\Installer::postCreateProject"
    ]
  },
  "extra": {
    "yii\\composer\\Installer::postCreateProject": {
      "setPermission": [
        {
          "runtime": "0777",
          "web/assets": "0777",
          "yii": "0755"
        }
      ],
      "generateCookieValidationKey": [
        "config/web.php"
      ]
    },
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
